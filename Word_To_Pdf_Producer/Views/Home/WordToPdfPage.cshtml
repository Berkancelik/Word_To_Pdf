@model WordToPdf
@{
    ViewData["Title"] = "WordToPdfPage";
}

<h1>WordToPdfPage</h1>

<div class="row">
    <div class="col-md-400">
        <form asp-action="WordToPdfPage" method="post" enctype="multipart/form-data">
            <div class="form-group">
                <label asp-for="WordFile" ></label>
                <input asp-for="WordFile" class="form-control" />
            </div>

                <div class="form-group">
                <label asp-for="Email"> </label>
                <input asp-for="Email" class="form-control" />
            </div>

            <div class="form-group">
                <input type="submit" value="Donüştür" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>
